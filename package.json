{
  "name": "shopping-cart",
  "version": "1.0.0",
  "main": "src/app.js",
  "license": "MIT",
  "scripts": {
    "start:dev": "nodemon",
    "start": "NODE_ENV=production node ./build/app.js",
    "create:db": "node_modules/.bin/sequelize db:create",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "migrate:reset": "node_modules/.bin/sequelize db:migrate:undo:all && yarn run migrate",
    "test:integration": "cross-env NODE_ENV=test yarn run create:db migrate:reset && jest -c jest.config.js ./test --runInBand --detectOpenHandles --forceExit"
  },
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/runtime": "^7.5.4",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "sequelize": "^6.5.0",
    "supertest": "^6.1.3",
    "validatorjs": "^3.22.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "jest-serial-runner": "^1.1.0",
    "mysql2": "^2.2.5",
    "sequelize-cli": "^6.2.0"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/test/utils/index.js"
    ]
  }
}
